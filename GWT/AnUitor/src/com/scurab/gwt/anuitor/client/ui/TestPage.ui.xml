<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.content{
			background-color: #DDffff;
			position:absolute;
			width:100%;
			height: 100%;
  			margin: 0;
  			padding: 0;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.content}">
		<g:SplitLayoutPanel width="100%" height="100%">
			<g:west size="768">
				<g:VerticalPanel height="100%">
					<g:FlowPanel ui:field="flowPanel">
						<g:Button text="Up" ui:field="btnUp"/>
						<g:Button text="Down" ui:field="btnDown"/>
						<g:Button ui:field="btnReload" text="Reload"/>
						<g:Button text="Test" ui:field="btnTest"/>
						<g:Label text="M:" ui:field="mousePosition"/>
						<g:Image ui:field="image" width="0" height="0" visible="false"/>
					</g:FlowPanel>
				</g:VerticalPanel>
			</g:west>
			<g:west size="768">
				<g:VerticalPanel height="100%" ui:field="testPanel">
					<g:Label text="New Label" ui:field="text"/>
					
				</g:VerticalPanel>
			</g:west>												
		</g:SplitLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 